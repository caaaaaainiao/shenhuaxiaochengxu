.buy {
  width: 100rpx;
  height: 50rpx;
  font-size: 24rpx;
  color: #DA4E7B;
  line-height: 48rpx;
  text-align: center;
  border: 1rpx solid #DA4E7B;
  position: absolute;
  left: 0;
  top: 40rpx;
  border-radius: 25rpx;
}

.session_bottom {
  width: 100%;
  line-height: 1;
  font-size: 22rpx;
  color: #888;
  height: 25rpx;
  display: block;
}

.price .session_bottom image {
  width: 32rpx;
  height: 32rpx;
  float: right;
  margin-top: -4rpx;
  margin-left: 6rpx;
}

.price .session_bottom text {
  float: right;
}

.price .session_bottom {
  padding-right: 43rpx;
}

.btns .session_bottom {
  width: 100rpx;
  text-align: center;
  position: absolute;
  left: 0;
  top: 107rpx;
  text-decoration: line-through;
}

.others {
  width: 100%;
  float: left;
  overflow: hidden;
}

.store {
  width: 100%;
  height: 100rpx;
  background: #f2f2f2;
  /* border-bottom: 1rpx solid #E5E5E5; */
  padding-left: 30rpx;
  line-height: 100rpx;
  font-size: 24rpx;
  color: #2e2d2d;
  position: relative;
}

.store image {
  width: 30rpx;
  height: 30rpx;
  margin-right: 18rpx;
  vertical-align: middle;
}

.others .buy {
  left: 614rpx;
  top: 25rpx;
}

.nowPrice {
  position: absolute;
  left: 453rpx;
  top: 0;
  height: 100%;
  color: #DA4E7B;
}

.nowPrice .num, .minPrice .num {
  font-size: 36rpx;
}

.session_top .minPrice {
  position: absolute;
  left: 39rpx;
  top: 0;
  height: 100%;
  font-size: 24rpx;
  color: #DA4E7B;
}

.node {
  width: 20rpx;
  height: 20rpx;
  background: #fff;
  position: absolute;
  top: 140rpx;
  left: 480rpx;
  transform: rotateZ(45deg);
  z-index: 9;
}
