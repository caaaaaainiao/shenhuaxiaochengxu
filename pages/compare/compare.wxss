.show{
  display: block
}

.hide{
  display: none
}

.container {
  width: 100%;
  height: 100%;
  position: relative;
  overflow: auto;
  /* background: red; */
  padding-bottom: 70rpx;
}

.bannerbg {
  width: 100%;
  height: 432rpx;
  position: relative;
  top: 0;
  left: 0;
}

.location {
  width: auto;
  height: 50rpx;
  background: rgba(1, 1, 1, 0.1);
  padding: 0 15rpx 0 10rpx;
  position: absolute;
  top: 10rpx;
  left: 20rpx;
  font-size: 24rpx;
  color: #fff;
  border-radius: 25rpx;
  box-sizing: border-box;
  line-height: 50rpx;
}

.location image {
  width: 40rpx;
  height: 40rpx;
  margin-top: 5rpx;
  margin-right: 5rpx;
  float: left;
}

