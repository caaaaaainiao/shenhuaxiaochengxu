.swiper-block {
  height: 400rpx;
  width: 100%;
  position: absolute;
  top: 0;
  left: 0;
}

.swiper-item {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: flex-start;
  overflow: unset;
}

.slide-image {
  height: 252rpx;
  width: 180rpx;
  border-radius: 9rpx;
  /* margin: 0rpx 30rpx; */
  z-index: 1;
}

.active {
  transform: scale(1.14);
  transition: all 0.2s ease-in 0s;
  z-index: 20;
}

.check {
  width: 20rpx;
  height: 20rpx;
  background: #fff;
  position: absolute;
  top: 390rpx;
  left: 365rpx;
  transform: rotateZ(45deg);
}

.movieDetail {
  width: 100%;
  height: 118rpx;
  padding-top: 15rpx;
  border-bottom: 1rpx solid #c9c9c9;
  box-sizing: border-box;
}

.movieName {
  line-height: 1;
  font-size: 30rpx;
  color: #312927;
  text-align: center;
  margin-bottom: 19rpx;
  font-weight: bold;
}

.movieMessage {
  line-height: 1;
  font-size: 24rpx;
  color: #888;
  text-align: center;
  padding: 0 20rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.movieMessage text{
  margin-right: 10rpx;
}

.dates {
  width: 100%;
  height: 77rpx;
  border-bottom: 1rpx solid #c9c9c9;
  padding-top: 20rpx;
  padding-left: 55rpx;
  white-space: nowrap;
}

.date {
  width: 180rpx;
  height: 57rpx;
  /* float: left; */
  margin-right: 50rpx;
  text-align: center;
  font-size: 24rpx;
  color: #312927;
  line-height: 55rpx;
  display: inline-block;
}

.select {
  color: #DA4E7B;
  border-bottom: 10rpx solid #DA4E7B;
}

.session {
  width: 100%;
  /* height: 150rpx; */
  overflow: hidden;
  position: relative;
  border-bottom: 1rpx solid #c9c9c9;
}

.time {
  width: 150rpx;
  height: 150rpx;
  float: left;
  margin-left: 44rpx;
}

.address {
  width: 220rpx;
  height: 150rpx;
  float: left;
}

.price {
  width: 200rpx;
  height: 150rpx;
  float: left;
}

.btns {
  width: 130rpx;
  height: 150rpx;
  float: left;
  position: relative;
}

.session_top {
  width: 100%;
  height: 67rpx;
  line-height: 100rpx;
  vertical-align: bottom;
  margin-bottom: 40rpx;
  position: relative;
}

.start_time {
  font-weight: bold;
  font-size: 36rpx;
  color: #312927;
}

.movie_type {
  font-size: 24rpx;
  color: #312927;
  text-overflow: ellipsis;
  overflow: hidden;
  white-space: nowrap
}

.session_top image {
  width: 24rpx;
  height: 24rpx;
  margin-right: 15rpx;
  float: left;
  margin-top: 43rpx;
}

